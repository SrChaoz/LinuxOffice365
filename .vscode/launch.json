{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Electron: Main",
        "program": "${workspaceFolder}/node_modules/electron/dist/electron.js",
        "args": ["."],
        "cwd": "${workspaceFolder}",
        "runtimeExecutable": "npm",
        "runtimeArgs": ["start"],
        "env": {
          "NODE_ENV": "development"
        },
        "outputCapture": "std"
      }
    ]
  }
  